    "class Student(object):\n",
    "    \n",
    "    @property\n",
    "    def birth(self):\n",
    "        return self._birth\n",
    "\n",
    "    @birth.setter\n",
    "    def birth(self, value):\n",
    "        self._birth = value\n",
    "\n",
    "    @property\n",
    "    def age(self):\n",
    "        return 2019 - self._birth\n",
    "    \n",
    "s = Student()\n",
    "s.birth=2000\n",
    "print(s.age)"
   ]
